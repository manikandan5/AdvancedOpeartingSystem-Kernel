/* xsh_memcmp.c - xsh_memcmp */

#include <xinu.h>
#include <string.h>
#include <stdio.h>

/*------------------------------------------------------------------------
 * xsh_memcmp - Assign Memory using Best Fit method
 *------------------------------------------------------------------------
 */
shellcmd xsh_memcmp(int nargs, char *args[]) {

	traverse();
	printf("After Assigning 100 bytes....\n");
	int* k = new_getstk(100);
	traverse();
	int* d = new_getstk(200);
	printf("After Assigning 200 bytes....\n");
	traverse();
	int* f = new_getstk(500);
	printf("After Assigning 500 bytes....\n");
	traverse();
	freemem(f,sizeof(f));
	printf("After freeing up  500 bytes....\n");
	traverse();
	return 0;
}
